{% if attribute.value['media_id'] is defined %}<div class="picture" style="display: block; margin: 0 10px 10px 0; float: left; clear: both"><img src="{{ teelMediaLoad(attribute.value.media_id, 200, 200, false, null, { 'src' : true})|raw }}" title="{{attribute.value['title']}}" onerror='this.src="{{ asset('bundles/majesmedia/img/icon-document.png')}}"'/><legend>{{attribute.value['author']}}</legend></div>{% endif %}